    <!-- https://www.openriskmanual.org/ns/nplo.owl#has_channel_of_origination -->

    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/nplo.owl#has_channel_of_origination">
        <rdfs:subPropertyOf rdf:resource="https://www.openriskmanual.org/ns/nplo.owl#Important"/>
        <rdfs:subPropertyOf rdf:resource="https://www.openriskmanual.org/ns/nplo.owl#NonConfidential"/>
        <rdfs:subPropertyOf rdf:resource="https://www.openriskmanual.org/ns/nplo.owl#Static"/>
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/nplo.owl#Loan"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Branch</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Broker</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Internet</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Other</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Channel through which the Loan was originated, i.e. Branch, Internet and Broker</rdfs:comment>
        <rdfs:comment>EBA 1.1 Field Index: 7.002</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/nplo.owl"/>
        <rdfs:label>has_channel_of_origination</rdfs:label>
        <rdfs:seeAlso>https://www.openriskmanual.org/wiki/EBA_NPL.Loan.Channel_of_Origination</rdfs:seeAlso>
        <owl:versionInfo>0.1</owl:versionInfo>
    </owl:DatatypeProperty>
    


